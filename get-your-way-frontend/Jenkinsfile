pipeline {
    agent any
    environment {
        CI = true
    }
        stages {
            stage('WhereamI') {
                steps {
                    sh 'pwd'
                    sh 'ls'
                }
            }
            stage('Install dependencies') {
                    steps {
                        sh 'ls'
                        sh 'pwd'
                        sh 'npm install'
                        sh 'npm run build'
                        sh 'npm start'
                    }
            }

        }
}